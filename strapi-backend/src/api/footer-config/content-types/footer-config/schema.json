      "enum": ["main", "startup-studio"],
      "required": true,
      "unique": true
    },
    "companyDescription": {
      "type": "text",
      "required": true
    },
    "servicesTitle": {
      "type": "string",
      "default": "Services"
    },
    "servicesLinks": {
      "type": "json",
      "required": true
    },
    "contactTitle": {
      "type": "string",
      "default": "Contact"
    },
    "contactLinks": {
      "type": "json",
      "required": true
    },
    "socialLinks": {
      "type": "json",
      "required": true
    },
    "copyrightText": {
      "type": "string",
      "required": true
    }
  }
}
